[Unit]
Description=import-officialevent-bat
After=network.target

[Service]
Type=oneshot
ExecStart=/home/ubuntu/vsrecorder/import-officialevent-bat/scripts/daily_batch.sh
WorkingDirectory=/home/ubuntu/vsrecorder/import-officialevent-bat

# 失敗時リトライ
Restart=on-failure
RestartSec=900
StartLimitInterval=5400
StartLimitBurst=5